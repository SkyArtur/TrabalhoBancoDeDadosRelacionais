table(class='table simple-table table-striped mt-2')
    thead(class='table-dark text-center')
        tr
            th(scope='col') ID
            th(scope='col') Nome
            th(scope='col') Telefone
            th(scope='col') Email
            th(scope='col') Endere√ßo
    tbody
        if elements
            each element in elements
                tr
                    td(scope='row'): input(class='form-check-input' type='radio' name='id' element_id value=`${element.id}`)
                    td(id=`nome${element.id}`) #{element.nome}
                    td(id=`telefone${element.id}`) #{element.telefone}
                    td(id=`email${element.id}`) #{element.email}
                    td(id=`endereco${element.id}`) #{element.endereco}